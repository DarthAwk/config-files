# a file containing all aliases used
# source this file in your bash_profile

# ruby aliases
alias be="bundle exec"
alias berdbm="bundle exec rake db:migrate"
alias ber="bundle exec rake"
alias bers="bundle exec rails server"
alias bedbc="bundle exec rails dbconsole"

alias gav='git commit -a --verbose'
alias gcv='git commit --verbose'
alias gd='git diff'
alias ge='git add . && git commit -a --verbose'
alias gs='git status'
